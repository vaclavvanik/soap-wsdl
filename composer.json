{
  "name": "vaclavvanik/soap-wsdl",
  "description": "An easy way to handle WSDL data with zero dependencies",
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/vaclavvanik/soap-wsdl",
  "keywords": [
    "soap",
    "wsdl"
  ],
  "authors": [
    {
      "name": "Václav Vaník",
      "email": "vaclav.vanik@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.3 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "VaclavVanik\\Soap\\Wsdl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VaclavVanikTest\\Soap\\Wsdl\\": "test/unit"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
